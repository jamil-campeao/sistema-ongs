{
  "type": "module",
  "scripts": {
    "dev": "nodemon server.js",
    "build": "tsc",
    "start": "node --import ./src/instrument.js server.js",
    "start:dev": "nodemon --exec \"node --import ./src/instrument.js server.js\"",
    "migrate-reset": "npx prisma migrate reset --force",
    "migrate-generate": "npx prisma generate && npx prisma migrate dev --name init",
    "migrate-dev": "npx prisma migrate dev",
    "migrate-prod": "npx prisma migrate deploy",
    "seed": "node prisma/seed.js",
    "test": "vitest"
  },
  "dependencies": {
    "@faker-js/faker": "^9.6.0",
    "@prisma/client": "^6.5.0",
    "@sentry/node": "^10.20.0",
    "@sentry/profiling-node": "^10.20.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "faker": "^6.6.6",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^7.0.10",
    "prom-client": "^15.1.3",
    "routes": "^2.1.0",
    "vercel": "^41.4.1"
  },
  "devDependencies": {
    "nodemon": "^3.1.9",
    "prisma": "^6.5.0",
    "supertest": "^7.1.4",
    "vitest": "^3.2.4"
  }
}
